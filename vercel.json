{
  "buildCommand": "npm run build",
  "outputDirectory": "frontend/build",
  "builds": [
    {
      "src": "backend/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/build/$1"
    }
  ],
  "functions": {
    "backend/app.py": {
      "runtime": "python3.9"
    }
  }
}